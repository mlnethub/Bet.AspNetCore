{
  "AzureVault": {
    "BaseUrl": "https://bet.vault.azure.net/"
  },

  "EnableHttpsRedirection": false,

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.ML": "Trace"
    }
  },
  "AllowedHosts": "*",

  "SchedulerJobs": {
    "RebuildMLModelScheduledJob": {
      "RunImmediately": false,
      // once a day at 8 am
      "CronSchedule": "0 8 * * *"
    }
  },

  "ApplicationInsights": {
    // vault
    "InstrumentationKey": "",
    "EnableEvents": true,
    "EnableTraces": true
  },

  //vault
  "AzureLogAnalytics": {
    "WorkspaceId": "",
    "AuthenticationId": ""
  }

}
