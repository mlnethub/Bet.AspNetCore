{

  "LetsEncrypt": {

    "AcmeAccount": {
      "domains": [ "temp.ngrok.io" ],
      "email": "s@gmail.com",
      "useStaging": false
      // "letsEncryptUri": ""
    },

    "AcmeOrder": {
      "KeyAlgorithm": "ES256",
      "ValidationDelay": 2,
      "ValidationRetry": 60
    },

    "Certificate": {
      "TimeUntilExpiryBeforeRenewal": "31.00:00:00",
      "TimeAfterIssueDateBeforeRenewal": null
    },

    "StorageBlobs": {
      "AcmeAccount": {
        "ContainerName": "dev-account"
      },
      "Certificates": {
        "ContainerName": "dev-certificates"
      }
    },

    "AzureAuthentication": {
      "AzureCloudInstance": "AzurePublic",
      "TenantId": "",
      "SubscriptionId": ""
    },

    "AcmeRenewalJob": {
      "RunImmediately": false,
      //"CronSchedule": "0/10 * * * * *",
      "CronSchedule": "0 */1 * * *", // every hour
      "NamedOptions": ""
    }
  },

  "AzureVault": {
    "BaseUrl": "https://bet.vault.azure.net/"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },

  "AllowedHosts": "*",

  "AzureStorage": {

    "DefaultAccount": {
      "Name": "betstorage"
    }
  }
}
